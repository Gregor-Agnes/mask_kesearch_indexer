{
  "name": "zwo3/mask_kesearch_indexer",
  "type": "typo3-cms-extension",
  "description": "ke_search Indexer for all Mask-Elements",
  "keywords": [
    "TYPO3",
    "extension",
    "ke_search",
    "mask",
    "indexer"
  ],
  "license": ["GPL-2.0-or-later"],
  "require": {
    "typo3/cms-core": "^9.5.26 || ^10.4.14",
    "teaminmedias-pluswerk/ke_search": "^3.6.0",
    "mask/mask": "^4.2 || ^6.6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Zwo3\\MaskKesearchIndexer\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zwo3\\MaskKesearchIndexer\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "mask_kesearch_indexer"
    }
  }
}
