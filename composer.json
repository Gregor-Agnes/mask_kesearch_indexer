{
  "name": "zwo3/mask_kesearch_indexer",
  "type": "typo3-cms-extension",
  "description": "ke_search Indexer for all Mask-Elements",
  "keywords": [
    "TYPO3",
    "extension",
    "ke_search",
    "mask",
    "indexer"
  ],
  "license": ["GPL-2.0-or-later"],
  "require": {
    "typo3/cms-core": "^11.5.0 || ^12.4.0",
    "tpwd/ke_search": "^4.2.0 || ^5.0",
    "mask/mask": "^7.0.21 || ^8.2"
  },
  "autoload": {
    "psr-4": {
      "Zwo3\\MaskKesearchIndexer\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zwo3\\MaskKesearchIndexer\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "mask_kesearch_indexer"
    }
  }
}
